<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="com.share.activity.entity.Activity" table="activity">
		<id name="aid" column="aid" length="16">
			<generator class="native"/>
		</id>
		<property name="uid" length="16" />
		<property name="phone" length="11" />
		<property name="picture" length="25" />
		<property name="title" length="40" />
		<property name="state" length="1" />
		<property name="aexplain" length="500"/>
		<property name="ptime" length="30"/>
		<property name="starttime" length="30"/>
		<property name="stoptime" length="30"/>
		<property name="number" length="16"/>
	
	</class>
</hibernate-mapping>